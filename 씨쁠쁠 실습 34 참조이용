#include <iostream>
using namespace std;

bool findmax(int a[], int& max) {
	int highest = 0;
	for (int i = 0; i < 6; i++) {
		if (highest < a[i])
			highest = a[i];
	}
	max = highest;
	return true;
}

int main() {
	cout << "2021107050 한주은" << endl;

	int n[] = { -13, 12, 65, -7, 3 };
	int max;
	if (findmax(n, max))
		cout << "최댓값은 " << max << endl;
}
